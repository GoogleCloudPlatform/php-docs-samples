{
    "require": {
        "google/cloud-functions-framework": "^1.0",
        "google/cloud-storage": "^1.23",
        "google/cloud-vision": "^2.0",
        "ext-imagick": "*"
    },
    "scripts": {
        "start": [
            "Composer\\Config::disableProcessTimeout",
